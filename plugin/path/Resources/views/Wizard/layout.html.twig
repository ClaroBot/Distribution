{% extends "ClarolineCoreBundle:Workspace:layout.html.twig" %}

{% block stylesheets %}
    {# Path Editor styles #}
    {% stylesheets debug=false filter='less, cssmin' output='vendor/innovapath/wizards.css'
        'packages/angular-ui-tree/dist/angular-ui-tree.min.css'
        '@InnovaPathBundle/Resources/styles/wizards.less'
    %}
    <link rel="stylesheet" media="screen" href="{{ asset_url }}" />
    {% endstylesheets %}

    {#
        Claroline CSS.
        loading it after permits to override the styles through theme.
        there is no better way to override a plugin styles for now
    #}
    {{ parent() }}
{% endblock %}

{% block section_panel %}
    {% block path_wizard %}{% endblock %}
{% endblock %}

{% block javascripts %}
    {# Claroline JS #}
    {{ parent() }}

    {# Translations #}
    <script src="{{ url('bazinga_jstranslation_js', { 'domain': 'path_wizards' }) }}"></script>
{% endblock %}
